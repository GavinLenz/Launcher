<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.ToontownLauncher.controllers.eventhandlers.InvalidLoginController"
      alignment="CENTER" spacing="10" prefHeight="300" prefWidth="600">

    <children>
        <TableView fx:id="errorTable" prefHeight="300" prefWidth="400" styleClass="table-view">
            <columns>
                <TableColumn fx:id="toonColumn" text="Toon" prefWidth="150"/>
                <TableColumn fx:id="reasonColumn" text="Reason" prefWidth="450"/>
            </columns>
        </TableView>
    </children>
</VBox>